      <div class="paginate_do">
              <% @friends.each do |friend| %> 
               <div class="col-xs-12 col-md-6 col-lg-4">
                <!-- START Widget Panel -->
                        <div class="widget panel">
                          <!-- panel ribbon -->
                            <a href="javascript:void(0);" class="panel-ribbon panel-ribbon-primary"><i class="ico-info"></i></a>
                          <!--/ panel ribbon -->
                          <!-- panel ribbon -->
                            <% @like = current_user.rels(type: :likes, between: friend).blank? ? true : false %>

                            <% if @like %>
                              <a href='/users/<%= friend.neo_id %>/likes' class='panel-ribbon-60 panel-ribbon panel-ribbon-teal pull-right' data-remote='true'><i class='ico-heart icon-white icon-font-22'></i></a>
                            <% else %>
                              <a href='/users/<%= friend.neo_id %>/likes' class='panel-ribbon-60 panel-ribbon panel-ribbon-teal pull-right' data-remote='true'><i class='ico-heart icon-red icon-font-22'></i></a>
                            <% end %>
                                                         
                          <!--/ panel ribbon -->

                            <!-- Thumbnail -->
                            <div class="thumbnail">
                                <!-- media -->
                                <div class="media">
                                    <a href="/users/<%= friend.neo_id %>/">
                                    <img src="<%= facebook.get_picture(friend.uid,:type => "square", height: 400 , width: 400) %>" height="100%">
                                  </a>
                                </div>
                                <!--/ media -->
                            </div>
                            <!--/ Thumbnail -->
                             <!-- meta -->
                                    <span class="meta bottom">
                                        <h4 class="semibold mt10 text-center"><%= friend.name %></h4>
                                    </span>
                            <!--/ meta -->
                            <!-- blockquote -->
                            <div class="m10-p10">
                                <blockquote class="mt5 text-muted text-center m0-p10">
                                    <p class="font-15">Truth can be stated in a thousand different ways, yet each one can be true.  </p>
                                </blockquote>
                            </div>
                                <!--/ blockquote -->
                            <!-- panel body -->
                            <div class="panel-body">
                                 <!-- Nav section -->
                                <ul class="nav nav-section nav-justified">
                                    <li>
                                        <div class="section">
                                            <h4 class="nm"><i class="ico-heart-empty icon-red icon-font-55"></i></h4>
                                            <h4 class="nm mt5">12.9k</h4>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="section">
                                            <h4 class="nm"><i class="ico-shield2 icon-green icon-font-55"></i></h4>
                                            <h4 class="nm mt5">129</h4>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="section">
                                            <h4 class="nm"><i class="ico-blog2 icon-blue icon-font-55"></i></h4>
                                            <h4 class="nm mt5">12</h4>
                                        </div>
                                    </li>
                                </ul>
                                <!--/ Nav section -->
                            </div>
                            <!--/ panel body -->
                        </div>
                        <!--/ END Widget Panel -->
          <!--/row-->
                  </div>
                <% end %>      
      </div>

      <script type="text/javascript">
        $(".panel-ribbon").click(function(){
          if ($(this).children("i").hasClass( "icon-red" )) {
            $(this).children("i").removeClass("icon-red");
            $(this).children("i").addClass("icon-white");
          } else{
            $(this).children("i").addClass("icon-red");
            $(this).children("i").removeClass("icon-white");
          };
        });

      </script>